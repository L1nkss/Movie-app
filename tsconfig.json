{
    "compilerOptions": {
        "outDir": "./public",
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es5",
        "jsx": "react",
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "baseUrl": "./",
        "paths": {
            "@components/*": ["src/components/*"],
            "@constants/*": ["src/constants/*"],
            "@redux/*": ["src/redux/*"]
        }
    },
    "exclude": [
        "node_modules",
        "dist"
    ]
}
